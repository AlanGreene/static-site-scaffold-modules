{
  "name": "static-site-scaffold",
  "version": "3.2.0",
  "description": "Eleventy based static site scaffolding",
  "main": "lib/11ty.config.js",
  "contributors": [
    "Sam Richard <snugug@google.com> (https://snugug.com)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chromeos/static-site-scaffold-modules.git"
  },
  "files": [
    "lib/**/*",
    "LICENSE",
    "README.md",
    "CODE_OF_CONDUCT.md",
    "config/**/*",
    ".github/CONTRIBUTING.md"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "pretest": "run-p lint",
    "test:lint": "eslint tests",
    "js:lint": "eslint lib tests",
    "lint": "NODE_ENV=production run-p test:lint js:lint",
    "test": "nyc ava",
    "fix": "eslint tests --fix"
  },
  "license": "Apache-2.0",
  "ava": {
    "files": [
      "tests/**/*"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "all": true
  },
  "peerDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "gulp": "^4.0.2",
    "rollup": "^1.19.4"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@emmetio/expand-abbreviation": "^0.7.3",
    "browser-sync": "^2.26.7",
    "config": "^3.2.4",
    "critical": "^2.0.0-23",
    "eleventy-plugin-local-respimg": "^0.1.4",
    "gulp-cached": "^1.1.1",
    "gulp-eslint": "^6.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "iso-639-1": "^2.1.0",
    "js-yaml": "^3.13.1",
    "lazypipe": "^1.0.2",
    "lodash.clonedeep": "^4.5.0",
    "markdown-it": "^9.1.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-attrs": "^3.0.1",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-figure": "^0.2.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-header-sections": "^1.0.0",
    "markdown-it-kbd": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-video": "^0.6.3",
    "node-sass": "^4.13.1",
    "npm-run": "^5.0.1",
    "prismjs": "^1.17.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-workbox-inject": "^1.0.1",
    "uslug": "^1.0.4"
  }
}
